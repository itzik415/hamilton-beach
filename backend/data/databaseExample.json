//insert into product_category_details table
INSERT INTO product_category_details(category, title, para_text) VALUES 
('grinders', 'פעולה ללא ידיים ומדידה מדויקת עד לקבלת טחינה ברמה המושלמת בשבילכם', 'מטחנות התבלינים של המילטון ביץ׳ בעלות הטחינה המותאמת הן האידיאליות ביותר עבור קפה ותבלינים עד לקבלת המרקם המדויק עבורכם בכל פעם. הגדרות מוסברות היטב המבטיחות תוצאות מושלמות וסימנים ברורים הנמצאים על הכוס אשר יבטיחו לכם מדידה מדוייקת להבטיח מדידה קפדנית. המטחנות שלנו קלות לשימוש ונקיון. הן מציעות טחינה ללא שימוש בידיים עם כיבוי אוטומטי, כמו גם מיקום מוסתר לאיכסון הכבל. חלק ממטחנות הקפה אף כוללות קערית נירוסטה מחוספסת נשלפת לניקוי קל במדיח כלים.'),
('food-processors', 'מעבדי המזון שלנו מפחיתים את זמן ההכנה עם מנועים חזקים ואביזרים קלים לניקוי','מעבדי המזון של המילטון ביץ׳ מסתמכים על מנועים חזקים כדי ליקצוץ, לפרוס, לגרוס ולערבב כמעט כל דבר שתוסיפו לקערה. מהירים וקלים לשימוש, ובעלי  רעיונות חכמים ומגוונים - במיוחד בדגמים בעלי פתח ההזנה הרחב אשר מאפשרים הכנסה של מזון בשלמותו. בעוד מעבדי מזון יכולים להוות מטרד כאשר מדובר על ניקיון המוצר, העיצוב הקפדני והזהיר שלנו מאפשר אחסנה נוחה של המוצרים בארון וניקיון רוב החלקים במדיח. ביחרו את גודל מעבד המזון שעובד ומתאים לצרכיכם האישיים בצורה הטובה ביותר. מעבדי המזון של המילטון ביץ׳ הם המוצרים האולטימטיביים, בין אם אתם מבשלים עבור שניים או בין אם אתם מבשלים עבור אירוע משפחתי'),
('waffle-makers', 'פריך, היישר מהאופה בהבלגי האישי שלכם', 'איפו וופלים פריכים בתוך דקות עם האוסף הפופולרי של המילטון ביץ׳ Waffle Makers. נורית הביקורת תאמר לכם כאשר הוופל שלכם מוכן לאכילה, ולוחיות ה nonstick שלנו יעשו את עבודת הוצאת הוופל מהמכשיר בצורה חלקה ומושלמת.
הWaffle Makers של המילטון ביץ׳ מיועדים לניקוי מהיר, ובדגמים נבחרים אשר כוללים: ציפוי nonstick וגומחה לשאריות הנוזל עושים את עבודת הניקוי לקלה במיוחד. את משטחי ה nonstick קל לנקות עם מטלית לחה, ובדגמים נבחרים אף ניתן לשלוף את הפלטות ולהכניסם לניקיון במדיח'),
('toasters', 'טוסטים פריכים בתוך דקות', 'הכינו טוסטים פריכים בתוך דקות עם אוסף הטוסטרים של המילטון ביץ׳. נורית הביקורת תאמר לכם כאשר הטוסטר הגיע לטמפרטורה הרצויה. לוחיות ה nonstick שלנו יעשו את עבודת הוצאת הטוסט קלה ובצורה חלקה ומושלמת. הטוסטרים של המילטון ביץ׳ מיועדים לניקוי מהיר. את משטחי ה nonstick קל לנקות עם מטלית לחה, ובדגמים נבחרים אף ניתן לשלוף את הפלטות ולהכניסם לניקיון במדיח'),
('juciers', 'עבור המיצים האיכותיים ביותר', 'המסחטות של המילטון ביץ׳ מושלמות בכדי להכין מיצים טריים, מלהיבים וכמובן בריאים. גזרים, תפוזים, תפוחים, כרוב וסלק הם רק תמצית משלל הפירות והירקות שבהם תוכלו להשתמש בכדי להכין את המיץ המושלם עבורכם. בידקו את המתכונים שלנו למיצים טבעיים ובריאים על מנת לקבל טעימה מיעילות המכשירים שלנו. כמו כל שאר המוצרים של המילטון ביץ גם המסחטות שלנו מציעות יעילות ושוב פעם יעילות. אנו מציעים דגמים עם מנועים רבי עוצמה המסוגלים לסחוט מיץ ממגוון של פירות וירקות. הפיה הגדולה מאפשרת יתרון נוסף אשר לא תצטרכו לחתוך את הפרי/ירק לפני אלא, תוכלו להכניס אותו בשלמותו ובכך לחסוך זמן ומאמץ נוספים בזמן ההכנה. ייתרון נוסף הוא שרוב החלקים כמובן בטוחים לשימוש במדיח. ביחרו מתוך שלל הדגמים שאנו מציעים ועוד מגוון של עיצובים אחרים אשר ייסיעו לכם להנות ממיצים בריאים וטעימים בכל עת'),
('multi-cookers', 'לשלל מאכלים עם שימוש במכשיר אחד בלבד', 'המולטי קוקר של המילטון ביץ׳ מושלם בכדי להכין מגוון רחב של מאכלים ותבשילים. אורז ומרקים הם רק תמצית משלל התבשילים והמאכלים אשר תוכלו להכין באמצעות המכשיר. בידקו את המתכונים שלנו למגוון המאכליםשלנו על מנת לקבל טעימה מיעילות המכשיר. כמו כל שאר המוצרים של המילטון ביץ גם המולטי קוקר שלנו מציע יעילות רבה. פנים המוצר עשוי שכבת nonestick אשר מונעת ממאכלים מסויימים להידבק למכשיר ובאותו הזמן כמובן גם נוכה מאוד לניקיון.');


CREATE TABLE product_category_details (
    id SERIAL PRIMARY KEY,
    category text NOT NULL,
    title text NOT NULL,
    para_text text NOT NULL
);

CREATE TABLE spare_parts(
    id SERIAL PRIMARY KEY,
    image_url text,
    product_model text NOT NULL,
    category text NOT NULL,
    part_model text NOT NULL,
    price integer NOT NULL,
    name text NOT NULL,
    hebrew_name text NOT NULL
);

CREATE TABLE products(
    id SERIAL PRIMARY KEY,
    type text NOT NULL,
    category text NOT NULL,
    price integer NOT NULL,
    hebrew_category text NOT NULL,
    last_price integer NOT NULL,
    short_description text NOT NULL,
    image_url text NOT NULL,
    model text NOT NULL
);

CREATE TABLE recipes(
    id SERIAL PRIMARY KEY,
    name text NOT NULL,
    imageUrl text NOT NULL,
    category text NOT NULL,
    level text NOT NULL,
    cookingtime text NOT NULL,
    time text NOT NULL,
    kosher text NOT NULL,
    type text NOT NULL
);

